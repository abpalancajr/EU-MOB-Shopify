{% comment %}ly_global_begin{% endcomment %}{% include 'ly-global' %}{% comment %}ly_global_end{% endcomment %}<header class="site-header taneasy sticky" role="banner">
  <div class="header-top">
    <div class="container">
      {% include 'header-top-all-brands' %}
    </div>
  </div>
  	
  <div class="header-bottom">
    <div class="container">
      <div class="header-panel">
        <div class="row">
          <h1 class="header-logo col-xs-4">
            <div class="menu-block visible-phone"><!-- start Navigation Mobile  -->
              <div id="showLeftPush"></div>
            </div><!-- end Navigation Mobile  -->
            <a href="/collections/tan-easy-shop">
              {% comment %}ly_asset_replace_for_[ 'taneasy-logo.png' | asset_url | img_tag: langify_shop_name ]_begin{% endcomment %}{% assign ly_asset = 'taneasy-logo.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | img_tag: langify_shop_name }}{% comment %}ly_asset_replace_end{% endcomment %}
              {% if settings.enable_retina %}
              	{% comment %}ly_asset_replace_for_[ 'taneasy-logo.png' | asset_url | img_tag: langify_shop_name ]_begin{% endcomment %}{% assign ly_asset = 'taneasy-logo.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | img_tag: langify_shop_name }}{% comment %}ly_asset_replace_end{% endcomment %}
              {% endif %}
            </a>
          </h1>
          <!-- <div class="nav-search on">{% include 'search-bar' %}</div> -->
        </div>
      </div>
    </div>
    
    <nav class="nav-bar" role="navigation">
      <div class="header-logo-fix">
        <a href="http://new.taneasyprotools.com">{% comment %}ly_asset_replace_for_[ 'taneasy-logo.png' | asset_url | img_tag: langify_shop_name ]_begin{% endcomment %}{% assign ly_asset = 'taneasy-logo.png' %}{% include 'ly-asset' with ly_asset %}{{ ly_translation  | img_tag: langify_shop_name }}{% comment %}ly_asset_replace_end{% endcomment %}</a>
      </div>
      <div class="container">
        {% include 'taneasy-nav' %}     
      </div>
    </nav>
  </div>
</header>